{
    "bootstrap_servers": ["localhost:9092"],
    "auto_offset_reset": "latest",
    "enable_auto_commit": "True",
    "group_id": "my-group",
    "value_deserializer": "lambda x: loads(x.decode('utf-8'))",
    "topics":[
        "input_topic"
    ],
    "model_alg": ["LSTM_model()"],
    "model_conf":[
        {
            "model_name": "alicante_autobuses",
            "horizon" : 1,
            "training_data": "./data_autobus.csv",
            "model_structure": {
                "n_of_neurons": 64,
                "n_of_timesteps": 24,
                "num_features": 1,
                "learning_rate": 0.01,
                "dropout": 0.1,
                "test_size": 0.25,
                "validation_split": 0.2,
                "epochs": 20,
                "batch_size": 128          
            },
            "output": [
                {"topic": "1h",
                "mask": 0},
                {"topic": "2h",
                    "mask": 1}
            ]
        }
    ]
}
